#!/bin/sh

#playbacker_cfg
#This file is responsible for initialization of all input(user-defined) variables required for each NE Type with respect to Stats and Events as one time activity for Playbacker Framework.
#{} mentioned for variables which user can defined as per requirement.
#NOTE: Please use # for comment.

###### Retrival of HostName of Server. No need to update. #######
HOST=`hostname`
HOST_NAME=`echo $HOST | sed s/-/_/g`

###### LIST OF CONSTANTS used in Playbacker ######
NE_TYPE_LIST="FrontHaul SBG-IS SAPC-TSP MTAS-TSP HSS-FE-TSP CSCF-TSP"
PM_DIR=/netsim/netsim_dbdir/simdir/netsim/netsimdir

FrontHaul_GENRATE_STATS=YES
FrontHaul_STATS_OUTPUT_FILE_FORMAT="ADATE.ROP_START_TIME-ROP_END_TIME.xml"
FrontHaul_STATS_INPUT_LOCATION=/netsim_users/pms/sandbox_templates/FrontHaul
FrontHaul_STATS_OUTPUT_TZ=LOCAL
FrontHaul_STATS_UPDATE_TZ=YES
FrontHaul_STATS_OUTPUT_TYPE=STAMPED
FrontHaul_STATS_APPEND_PATH=/c/pm_data

SBG_IS_GENRATE_STATS=YES
SBG_IS_STATS_OUTPUT_FILE_FORMAT="ADATE.ROP_START_TIME-ROP_END_TIME_-1_UNIQUE_ID.xml"
SBG_IS_STATS_INPUT_LOCATION=/netsim_users/pms/sandbox_templates/SBGIS
SBG_IS_STATS_OUTPUT_TZ=LOCAL
SBG_IS_STATS_UPDATE_TZ=NO
SBG_IS_STATS_OUTPUT_TYPE=COPY
SBG_IS_STATS_APPEND_PATH=/c/pm_data

HSS_FE_TSP_GENRATE_STATS=YES
HSS_FE_TSP_STATS_OUTPUT_FILE_FORMAT="ADATE.ROP_START_TIME-ROP_END_TIME_MANAGED_ELEMENT_ID_MEASUREMENT_JOB_NAME.xml"
HSS_FE_TSP_STATS_INPUT_LOCATION=/netsim_users/pms/sandbox_templates/TSP/HSS_FE
HSS_FE_TSP_STATS_OUTPUT_TZ=UTC
HSS_FE_TSP_STATS_UPDATE_TZ=NO
HSS_FE_TSP_STATS_OUTPUT_TYPE=COPY
HSS_FE_TSP_STATS_APPEND_PATH=/opt/telorb/axe/tsp/NM/PMF/reporterLogs
HSS_FE_TSP_MEASUREMENT_JOB_NAME="Diameter_error_counters Diameter_traffic_counters HSS-AVG HSS-ESM HSS-ISM HSS-SDA HSS-SLF"

CSCF_TSP_GENRATE_STATS=YES
CSCF_TSP_STATS_OUTPUT_FILE_FORMAT="ADATE.ROP_START_TIME-ROP_END_TIME_MANAGED_ELEMENT_ID_MEASUREMENT_JOB_NAME.xml"
CSCF_TSP_STATS_INPUT_LOCATION=/netsim_users/pms/sandbox_templates/TSP/CSCF
CSCF_TSP_STATS_OUTPUT_TZ=UTC
CSCF_TSP_STATS_UPDATE_TZ=NO
CSCF_TSP_STATS_OUTPUT_TYPE=COPY
CSCF_TSP_STATS_APPEND_PATH=/opt/telorb/axe/tsp/NM/PMF/reporterLogs
CSCF_TSP_MEASUREMENT_JOB_NAME="cscf cscf2"

MTAS_TSP_GENRATE_STATS=YES
MTAS_TSP_STATS_OUTPUT_FILE_FORMAT="ADATE.ROP_START_TIME-ROP_END_TIME_MANAGED_ELEMENT_ID_MEASUREMENT_JOB_NAME.xml"
MTAS_TSP_STATS_INPUT_LOCATION=/netsim_users/pms/sandbox_templates/TSP/MTAS
MTAS_TSP_STATS_OUTPUT_TZ=UTC
MTAS_TSP_STATS_UPDATE_TZ=NO
MTAS_TSP_STATS_OUTPUT_TYPE=COPY
MTAS_TSP_STATS_APPEND_PATH=/opt/telorb/axe/tsp/NM/PMF/reporterLogs
MTAS_TSP_MEASUREMENT_JOB_NAME="MtasTraf"

SAPC_TSP_GENRATE_STATS=YES
SAPC_TSP_STATS_OUTPUT_FILE_FORMAT="ADATE.ROP_START_TIME-ROP_END_TIME_MANAGED_ELEMENT_ID_MEASUREMENT_JOB_NAME.xml"
SAPC_TSP_STATS_INPUT_LOCATION=/netsim_users/pms/sandbox_templates/TSP/SAPC
SAPC_TSP_STATS_OUTPUT_TZ=UTC
SAPC_TSP_STATS_UPDATE_TZ=NO
SAPC_TSP_STATS_OUTPUT_TYPE=COPY
SAPC_TSP_STATS_APPEND_PATH=/opt/telorb/axe/tsp/NM/PMF/reporterLogs
SAPC_TSP_MEASUREMENT_JOB_NAME="PlatformMeasures"
